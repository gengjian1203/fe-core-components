{
  "name": "fe-cosx-ui",
  "version": "1.0.0",
  "description": "Professional React component library with React 19, Tailwind CSS, and Storybook",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -c rollup.config.js -w",
    "dev": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "commit": "cz",
    "prepublishOnly": "pnpm build",
    "analyze": "pnpm build && npx bundlesize",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "release:patch": "node scripts/release-check.js && npm version patch && git push && git push --tags",
    "release:minor": "node scripts/release-check.js && npm version minor && git push && git push --tags",
    "release:major": "node scripts/release-check.js && npm version major && git push && git push --tags",
    "prerelease": "pnpm lint:check && pnpm typecheck && pnpm build",
    "release:check": "node scripts/release-check.js"
  },
  "keywords": [
    "react",
    "component-library",
    "typescript",
    "tailwind",
    "storybook",
    "ui-components"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^6.0.1"
  },
  "peerDependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "antd": "^5.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.28.3",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@eslint/js": "^9.35.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-a11y": "^9.1.4",
    "@storybook/addon-docs": "^9.1.4",
    "@storybook/addon-interactions": "9.0.0-alpha.10",
    "@storybook/addon-links": "^9.1.4",
    "@storybook/react": "^9.1.4",
    "@storybook/react-vite": "^9.1.4",
    "@storybook/test": "9.0.0-alpha.2",
    "@types/node": "^24.3.1",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "autoprefixer": "^10.4.21",
    "bundlesize": "^0.18.2",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^4.1.0",
    "cssnano": "^6.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.10.2",
    "globals": "^16.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rollup": "^4.50.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "storybook": "^9.1.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,md}": [
      "prettier --write"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/esm/index.js",
      "maxSize": "50kb"
    },
    {
      "path": "./dist/cjs/index.js",
      "maxSize": "55kb"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/fe-cosx-ui.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/fe-cosx-ui/issues"
  },
  "homepage": "https://github.com/your-org/fe-cosx-ui#readme",
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
